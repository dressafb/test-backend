{
    "name": "dominio/search-product",
    "description": "busca produtos pela api do vtex",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "aws/aws-sdk-php": "^3.164",
        "beberlei/doctrineextensions": "^1.2.9",
        "ekino/newrelic-bundle": "^2.2",
        "guzzlehttp/guzzle": "^7.2",
        "lcobucci/error-handling-middleware": "^1.1",
        "nesbot/carbon": "^2.41.5",
        "ramsey/uuid": "^4.1.1",
        "scienta/doctrine-json-functions": "^4.1",
        "symfony/asset": "5.1.*",
        "symfony/console": "5.1.*",
        "symfony/dotenv": "5.1.*",
        "symfony/expression-language": "5.1.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.1.*",
        "symfony/messenger": "5.1.*",
        "symfony/mime": "5.1.*",
        "symfony/monolog-bundle": "^3.6",
        "symfony/orm-pack": "^2.0",
        "symfony/serializer-pack": "^1.0",
        "symfony/twig-bundle": "5.1.*",
        "symfony/validator": "5.1.*",
        "symfony/yaml": "5.1.*",
        "ext-json": "*"
    },
    "require-dev": {
        "doctrine/coding-standard": "^8.2",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^0.12.25",
        "phpstan/phpstan-doctrine": "^0.12.14",
        "phpunit/phpunit": "^9",
        "psalm/plugin-phpunit": "^0.13.0",
        "psalm/plugin-symfony": "^2.0.2",
        "squizlabs/php_codesniffer": "3.5.5",
        "symfony/browser-kit": "5.1.*",
        "symfony/maker-bundle": "^1.19",
        "symfony/profiler-pack": "^1.0",
        "vimeo/psalm": "^4.2.1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "process-timeout": 0
    },
    "autoload": {
        "psr-4": {
            "Framework\\": "src/Framework/",
            "Dominio\\": "src/",
            "Tests\\": "tests/"
        },
        "files": [
            "src/Kernel.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Framework\\": [
                "tests/unit/Framework/"
            ],
            "Tests\\": [
                "tests/unit",
                "tests/functional"
            ]
        },
        "files": []
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "test": [
            "phpcs",
            "phpstan analyse --memory-limit=-1",
            "phpunit"
        ],
        "lint": [
            "phpcs"
        ],
        "lint-fix": [
            "phpcbf"
        ],
        "coverage": [
            "phpunit --coverage-html var/coverage/"
        ],
        "worker:queue:consumer:webhook": [
            "console messenger:consume webhook-dispatcher-async --time-limit=600 -vvv --no-interaction"
        ],
        "worker:queue:consumer": [
            "console messenger:consume async --time-limit=600 -vvv --no-interaction"
        ],
        "worker:queue:producer": [
            "console messenger:consume async --time-limit=600 -vvv --no-interaction"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.1.*"
        },
        "phpstan": {
            "includes": [
                "extension.neon"
            ]
        }
    }
}
